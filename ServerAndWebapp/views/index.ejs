<!DOCTYPE html>
<html ng-app="iotGarden">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="IOT Garden webapp">
    <meta name="author" content="@shenrgc">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap-theme.min.css">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.6.3/css/font-awesome.min.css">
    <link rel="stylesheet" href="stylesheets/style.css" type="text/css" />
    <title>IOT Garden</title>
  </head>
  <body>
    <noscript>
        Please enable Javascript to experience this site :)
    </noscript>
    <div ng-controller="generalController">
      <div id="content-page" ui-view></div>
    </div>
    <!-- Libraries  -->
    <script src="https://code.jquery.com/jquery-3.0.0.min.js"></script>
    <script src="https://code.jquery.com/ui/1.11.4/jquery-ui.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.5.7/angular.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/angular-ui-router/0.3.1/angular-ui-router.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/angular.js/1.5.7/angular-cookies.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/angular-ui-bootstrap/1.3.2/ui-bootstrap-tpls.min.js"></script>
    <!-- Plot -->
    <script src="scripts/jquery.flot.js"></script>
    <script src="scripts/jquery.flot.time.js"></script>
    <script src="scripts/jquery.flot.resize.js"></script>
    <script src="scripts/jquery.flot.tooltip.min.js"></script>
    <!-- Configuration -->
    <script src="app/main.js"></script>
    <!-- Controllers -->
    <script src="app/controllers/generalCtrl.js"></script>
    <script src="app/controllers/loginCtrl.js"></script>
    <script src="app/controllers/dashboardCtrl.js"></script>
    <!-- Services -->
    <script src="app/services/mainService.js"></script>
    <script src="app/services/gardenService.js"></script>
    <!-- End build -->
  </body>
</html>
